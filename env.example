# FreshTomato Unblock - Production Environment Variables

# SÉCURITÉ - OBLIGATOIRE  
SESSION_SECRET=yoursecret
# IMPORTANT: Double the $ signs for Docker Compose
ADMIN_PASSWORD_HASH=yourhash

# Configuration application
DB_PATH=/app/data/app.db
SSH_KEYS_PATH=/app/data/ssh_keys
NODE_ENV=production
PORT=3000

# Configuration SSH par défaut
DEFAULT_ROUTER_IP=192.168.1.1
DEFAULT_SSH_USER=root
DEFAULT_TIMEOUT_MINUTES=1
MAX_TIMEOUT_MINUTES=10

# Add this line to your .env file to force HTTP cookies
FORCE_HTTPS=false