# Copier ce fichier vers .env et modifier les valeurs

# SÉCURITÉ - OBLIGATOIRE
SESSION_SECRET=your_session_secret_key_at_least_32_chars_long_minimum
ADMIN_PASSWORD_HASH=your_bcrypt_hash_here

# DÉVELOPPEMENT UNIQUEMENT (ne pas utiliser en production)
# ADMIN_PASSWORD=your_secure_admin_password

# Configuration application
DB_PATH=/app/data/app.db
SSH_KEYS_PATH=/app/data/ssh_keys
NODE_ENV=production
PORT=3000

# Configuration SSH par défaut (modifiable via interface admin)
DEFAULT_ROUTER_IP=192.168.1.1
DEFAULT_SSH_USER=root
DEFAULT_TIMEOUT_MINUTES=1
MAX_TIMEOUT_MINUTES=60
